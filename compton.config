inactive-dim = 0.2
fade-in-step = 0.5;
fade-out-step = 0.5;
menu-opacity = 1;
unredir-if-possible = false;
opacity-rule = [ "95:class_g *?= 'gnome-terminal'" ];
focus-exclude = [
        "_NET_WM_NAME@:s = 'rofi'",
        "class_g = 'google-chrome'",
        "class_g = 'Google-chrome'",
        "class_g = 'code'",
        "class_g = 'Code'",
        "class_g = 'Telegram'",
        "class_g = 'TelegramDesktop'"
]
